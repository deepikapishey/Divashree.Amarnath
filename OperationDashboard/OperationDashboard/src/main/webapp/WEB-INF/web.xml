        <servlet-name>UsersServlet</servlet-name>
        <url-pattern>/userss</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>WorkersServlet</servlet-name>
        <servlet-class>com.operationsorchestration.WorkersServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>WorkersServlet</servlet-name>
        <url-pattern>/workerss</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>RolesServlet</servlet-name>
        <servlet-class>com.operationsorchestration.RolesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RolesServlet</servlet-name>
        <url-pattern>/roless</url-pattern>
    </servlet-mapping>
</web-app>
